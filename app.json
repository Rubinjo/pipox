{
  "expo": {
    "name": "pipox",
    "description": "A privacy focussed messaging app",
    "slug": "pipox",
    "version": "1.0.0",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "icon": "./assets/pipox.png",
    "splash": {
      "image": "./assets/pipox_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#414141"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/pipox.png",
        "backgroundColor": "#414141"
      }
    }
  }
}
